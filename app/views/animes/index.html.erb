<h1>Animes#index</h1>
<div class="listeAnime">
  <ul class="items">
    <% @animes.each do |a| %>
    <li>
      <div class="itemsBorder">
        <div class="img">
          <%= link_to a do %>
            <img height=300px, width=180px, alt="<%=a.name%>Img", src=<%=a.img%>></img>
          <% end %>
        </div>
        <p class="name">
          <%= link_to a.name, a, class: "myLink"%>
        </p>
      </div>
    </li>
    <% end %>
  </ul>
</div>
